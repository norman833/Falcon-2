<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
    <session name="DLD1"
             role="initiator"
             no_chksum="true"
             fix_version="1100"
             active="true"
             ip="127.0.0.1"
             port="11002"
             sender_comp_id="DLD_TEX"
             target_comp_id="TEX_DLD"
             login_retry_interval="15000"
             heartbeat_interval="30"
             reset_sequence_numbers="false"
             tcp_nodelay="false"
             login_retries="100"
             tcp_recv_buffer="100663296"
             tcp_send_buffer="100663296"
             process_model="coroutine"
             persist="mem0" />

    <persist name="bdb0"
             type="bdb" dir="./run"
             db="hf_client.db" />

    <persist name="mem0"
             type="mem" />

    <log 		name="session_log"
                type="session"
                filename="|/bin/cat"
                rotation="5"
                flags="timestamp|sequence|compress|thread"/>

    <log 		name="protocol_log"
                type="protocol"
                filename="./run/hf_client2_protocol.log"
                rotation="5"
                flags="append|inbound|outbound"/>

    <log 		name="protocol_log_null"
                type="protocol"
                filename="/dev/null"/>
</fix8>